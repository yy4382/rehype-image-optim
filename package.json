{
  "name": "rehype-image-optim",
  "version": "0.0.1",
  "description": "rehype plugin to change the `src` (and `srcset`/`sizes`) of images to optimized versions from your image CDN/optimization provider",
  "scripts": {
    "dev": "tsc --watch",
    "build": "tsc",
    "prepare": "husky",
    "format": "prettier --write .",
    "lint": "eslint .",
    "check-types": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "rehype",
    "image",
    "optimization",
    "cdn"
  ],
  "author": "Yunfi",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": "./dist/lib/index.js"
  },
  "main": "./dist/lib/index.js",
  "dependencies": {
    "unist-util-visit": "^5.0.0"
  },
  "devDependencies": {
    "@types/eslint__js": "^8.42.3",
    "@types/hast": "^3.0.4",
    "eslint": "^9.9.0",
    "husky": "^9.1.4",
    "lint-staged": "^15.2.9",
    "prettier": "^3.3.3",
    "rehype": "^13.0.0",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.1.0",
    "vitest": "^2.0.5"
  }
}
